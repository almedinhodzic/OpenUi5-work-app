<mvc:View xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc" controllerName="sap.btp.myUI5App.controller.Home">
	<Shell>
		<App>
			<pages>
				<Page title="{i18n>homePageTitle}">
					<content>
						<Text text="{i18n>homePageContentText}" class="sapUiResponsiveMargin" width="auto" />
						<Button text="{i18n>homePageButtonText}" press=".onNavToLogin" type="Emphasized"/>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>


</mvc:View>
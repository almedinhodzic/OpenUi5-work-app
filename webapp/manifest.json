{
  "_version": "1.21.0",
  "sap.app": {
    "id": "sap.btp.myUI5App",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "sap.btp.myUI5App.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.60.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap.btp.myUI5App.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "sap.btp.myUI5App.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": "notFound"
        },
        "async": true
      },
      "routes": [
        {
          "name": "RouteApp",
          "pattern": "RouteApp",
          "target": ["TargetApp"]
        },
        {
          "name": "Home",
          "pattern": "",
          "target": "home"
        },
        {
          "name": "login",
          "pattern": "login",
          "target": "login"
        },
        {
          "name": "dashboard",
          "pattern": "dashboard",
          "target": "dashboard"
        },
        {
          "name": "user",
          "pattern": "user",
          "target": "user"
        }
      ],
      "targets": {
        "TargetApp": {
          "viewType": "XML",
          "viewLevel": 1,
          "viewId": "app",
          "viewName": "App"
        },
        "home": {
          "viewType": "XML",
          "viewLevel": 2,
          "viewId": "home",
          "viewName": "Home"
        },
        "login": {
          "viewType": "XML",
          "viewLevel": 3,
          "viewId": "login",
          "viewName": "Login"
        },
        "dashboard": {
          "viewType": "XML",
          "viewLevel": 4,
          "viewId": "dashboard",
          "viewName": "Dashboard"
        },
        "user": {
          "viewType": "XML",
          "viewLevel": 5,
          "viewId": "user",
          "viewName": "User"
        },
        "notFound": {
          "viewId": "notFound",
          "viewName": "NotFound",
          "transition": "show",
          "viewType": "XML"
        }
      }
    }
  }
}
